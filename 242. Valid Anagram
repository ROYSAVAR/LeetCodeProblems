class Solution {
public:
    bool isAnagram(string s, string t) {

        unordered_map<char, int> mymapS;
        unordered_map<char, int> mymapT;

        if( s.length() != t.length()) 
        return false; 

        for(int i = 0; i<s.length(); i++){
            mymapS[s[i]]++;
            mymapT[t[i]]++;
        }

        return mymapS == mymapT ;

        
    }
};
