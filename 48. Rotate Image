class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {

        int n = matrix.size();           // n√∫mero de filas 
       

        // hacer transpuesta
        for(int i = 0; i < n; i++ )
        {
            for(int j = i + 1; j < n; j++ ){
             swap(matrix[i][j], matrix[j][i]);

            }
        }

        // rotar la matriz horizontal 
        
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n / 2; j++) {
                swap(matrix[i][j], matrix[i][n - j - 1]);
            }
        }
 

        
    }
};
